app: chrome
-
wave accessibility:key("ctrl-shift-u")
devtools:key("f12")
toggle device mode:key("ctrl-shift-m")

clear console:key("ctrl-l")
last panel:key("ctrl-[")
next panel:key("ctrl-]")

search (file | page):key("ctrl-f")
tidy search:
    key("ctrl-f")
    key("ctrl-a")
    key("backspace")
    key("escape")